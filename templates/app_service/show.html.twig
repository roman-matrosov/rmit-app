{% extends 'base.html.twig' %}

{% block title %}AppService{% endblock %}

{% block body %}
    <h1>AppService</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ app_service.id }}</td>
            </tr>
            <tr>
                <th>ServiceCode</th>
                <td>{{ app_service.serviceCode }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ app_service.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ app_service.type }}</td>
            </tr>
            <tr>
                <th>SubType</th>
                <td>{{ app_service.subType }}</td>
            </tr>
            <tr>
                <th>LastModified</th>
                <td>{{ app_service.lastModified ? app_service.lastModified|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ app_service.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_service_index') }}">back to list</a>

    <a href="{{ path('app_service_edit', {'id': app_service.id}) }}">edit</a>

    {{ include('app_service/_delete_form.html.twig') }}
{% endblock %}
