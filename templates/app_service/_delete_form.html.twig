<form method="post" action="{{ path('app_service_delete', {'id': app_service.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ app_service.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
